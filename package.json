    "@eslint/js": "^9.15.0",
    "cypress": "^13.16.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-xpath": "^2.0.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.15.0",
